        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "Properties": {
      "Application": "DDD.MrpDanu"
    }
  },
  "AllowedHosts": "*",
  "DatabaseSettings": {
    "MaxRetryCount": 10,
    "MaxRetryDelay": 30,
    "StandardExecutedDbCommandTime": 1000,
    "ConnectionString": "Server=tcp:samphamdb.database.windows.net,1433;Initial Catalog=DDDMrp;Persist Security Info=False;User ID=sampham;Password=DatabaseResearch123;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
  }
